{
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.fontSize": 14,
  "editor.cursorSurroundingLines": 5,
  // vim
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block-outline",
  "vim.cursorStylePerMode.visual": "block",
  "vim.leader": " ",
  "vim.highlightedyank.enable": true,
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "editor.fontFamily": "'Fira Code',Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "vim.cursorStylePerMode.replace": "underline",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "block",
  "vim.easymotion": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "F"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "W"],
      "commands": ["workbench.action.files.saveFiles"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    }
  ],
"editor.wordWrap": "on",
  "highlight-matching-tag.highlightFromContent": true,
  "terminal.integrated.fontSize": 14,
"terminal.external.osxExec": "iTerm.app",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.accessibilitySupport": "off",
"vsicons.dontShowNewVersionMessage": true,
  "editor.detectIndentation": false,
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 4,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.tabSize": 4
  },
  "python.linting.pylintArgs": ["--disable=C0111"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
},
"search.showLineNumbers": true,
"editor.lineNumbers": "relative",
"[lua]": {
    "editor.defaultFormatter": "Koihik.vscode-lua-format"
},
"git.openRepositoryInParentFolders": "never",
"git.autofetch": true,
"git.confirmSync": false,
"[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
},
"window.zoomLevel": -1,
"files.autoSave": "onFocusChange",
"git.confirmEmptyCommits": false
}
